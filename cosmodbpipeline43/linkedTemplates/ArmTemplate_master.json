{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cosmodbpipeline43"
		},
		"CosmosDbNoSql1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbNoSql1'"
		},
		"ls_cosmodb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_cosmodb'"
		},
		"ls_target_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_target'"
		},
		"ls_targetstorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_targetstorage'"
		},
		"ls_target_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cosmodbstorage67.dfs.core.windows.net/"
		},
		"ls_targetstorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cosmodbstorage67.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "cosmodbpipeline43_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CosmosDbNoSql1_connectionString": {
						"value": "[parameters('CosmosDbNoSql1_connectionString')]"
					},
					"ls_cosmodb_connectionString": {
						"value": "[parameters('ls_cosmodb_connectionString')]"
					},
					"ls_target_accountKey": {
						"value": "[parameters('ls_target_accountKey')]"
					},
					"ls_targetstorage_accountKey": {
						"value": "[parameters('ls_targetstorage_accountKey')]"
					},
					"ls_target_properties_typeProperties_url": {
						"value": "[parameters('ls_target_properties_typeProperties_url')]"
					},
					"ls_targetstorage_properties_typeProperties_url": {
						"value": "[parameters('ls_targetstorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}